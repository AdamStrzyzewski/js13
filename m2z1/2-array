// array

const arr = [1, 2, 3];
const arr2 = ["a", "b"];
const arr3 = [1, "a", null];

// n-dimensions array
// 3-dimension array
const arr4 = [
  [1, 2, [7, 8, 9]],
  [4, 5, 6],
];

console.log(arr[0]);
console.log(arr[arr.length - 1]);
console.log(arr4[0]);
console.log(arr4[0][2][0]);

