// functions arguments

function concat() {
  const concatArr = []
  // i, j, k
  // x, y, z
  // i, ii, iii
  for (let i = 0; i < arguments.length; i++) {
    for (let ii = 0; ii < arguments[i].length; ii++) {
      concatArr.push(arguments[i][ii])
    }
  }

  return concatArr
}

const arr1 = [1, 2, 3]
const arr2 = [4, 5, 6]
const arr3 = [7, 8, 9]

console.log(concat(arr1, arr2, arr3))